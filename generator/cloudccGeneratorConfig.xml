		<sqlMapGenerator targetPackage="sqlmaps"
			targetProject="C:/workspace3/cloudcc/config">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>


		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.alics.cms.mapper" targetProject="C:/workspace3/cloudcc/src/main/java">
